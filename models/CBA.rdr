Model CBA;

//Objectives

Objective Max ENB = EV(NB);
Objective Min LP = Pr(NB < 0);

//Parameters


//Equations

NB = Benefit - Cost;

//Decisions

Benefit = decision("Architecture Type") {
	"As-is": normalCI(0.9,1.1);
	"Refactoring": normalCI(1,9);
}

Cost = decision("Architecture Type") {
	"As-is": normalCI(0,0);
	"Refactoring": normalCI(1,5);
}

